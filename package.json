{
  "name": "aliemcards",
  "version": "0.5.22",
  "description": "ALiEM Cards deployment scripts and website code",
  "license": "MIT",
  "main": "./dist/server/index.js",
  "author": "Jeremy Voros",
  "scripts": {
    "build": "NODE_ENV=production webpack && gulp __build",
    "deploy": "torus run ./deploy",
    "dev": "webpack && gulp __build && ts-node ./dist/server/index.js",
    "lint": "npm run lint:typescript && npm run lint:stylus && npm run lint:markdown",
    "lint:markdown": "remark -q ./cards",
    "lint:stylus": "stylint app/assets/css",
    "lint:typescript": "tslint -p . -t codeFrame",
    "now-build": "echo build step skipped",
    "pretest": "npm run lint",
    "s3": "aws s3 sync ./cards s3://aliemcards --acl public-read --exclude \".*\" --exclude \"*/.*\" --exclude \"*/*.md\" --delete",
    "start": "ts-node ./dist/server/index.js",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run build && npm run test -- --no-coverage"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "now": {
    "alias": [
      "aliemcards.com",
      "www.aliemcards.com"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "files": [
      "dist"
    ],
    "name": "aliemcards"
  },
  "jest": {
    "globals": {
      "__TEST__": true
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*\\.(ts|tsx))$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json"
    ],
    "setupFiles": [
      "./before-test.ts"
    ],
    "verbose": true,
    "collectCoverageFrom": [
      "app/**/*.{ts,tsx}",
      "!**/__tests__/**",
      "!app/**/index.{ts,tsx}",
      "!app/App.jsx",
      "!app/utils/**"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.40",
    "apollo-client": "^2.2.5",
    "apollo-client-preset": "^1.0.8",
    "apollo-engine": "^0.8.10",
    "apollo-server-express": "^1.3.2",
    "autotrack": "^2.4.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "express": "^4.16.2",
    "fuse.js": "^3.2.0",
    "graphql": "^0.13.1",
    "graphql-tag": "^2.8.0",
    "graphql-tools": "^2.21.0",
    "helmet": "^3.11.0",
    "jest-cli": "^22.4.2",
    "normalizr": "^3.2.4",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "remark": "^9.0.0",
    "remark-html": "^7.0.0",
    "typescript": "^2.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@types/compression": "0.0.35",
    "@types/express": "^4.11.1",
    "@types/express-graphql": "0.0.36",
    "@types/helmet": "^0.0.37",
    "@types/jest": "^22.1.4",
    "@types/react": "^16.0.39",
    "@types/webpack": "^3.8.8",
    "autoprefixer-stylus": "^0.14.0",
    "awesome-typescript-loader": "^3.5.0",
    "babel-loader": "^8.0.0-beta.2",
    "css-loader": "^0.28.10",
    "del": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "front-matter": "^2.3.0",
    "gulp": "^4.0.0",
    "gulp-image": "^4.3.0",
    "gulp-stylus": "^2.7.0",
    "html-webpack-plugin": "github:webpack-contrib/html-webpack-plugin",
    "husky": "^0.15.0-rc.8",
    "jest": "^22.4.2",
    "js-yaml": "^3.10.0",
    "lint-staged": "^7.0.0",
    "now": "^10.1.0",
    "offline-plugin": "^4.9.0",
    "prettier": "^1.11.1",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.0.0",
    "react-svg-loader": "^2.1.0",
    "react-test-renderer": "^16.2.0",
    "remark-cli": "^5.0.0",
    "remark-lint": "^6.0.1",
    "remark-lint-blockquote-indentation": "^1.0.1",
    "remark-lint-emphasis-marker": "^1.0.1",
    "remark-lint-file-extension": "^1.0.1",
    "remark-lint-final-newline": "^1.0.1",
    "remark-lint-first-heading-level": "^1.1.1",
    "remark-lint-heading-increment": "^1.0.1",
    "remark-lint-heading-style": "^1.0.1",
    "remark-lint-linebreak-style": "^1.0.1",
    "remark-lint-list-item-bullet-indent": "^1.0.1",
    "remark-lint-list-item-content-indent": "^1.0.1",
    "remark-lint-list-item-indent": "^1.0.1",
    "remark-lint-no-auto-link-without-protocol": "^1.0.1",
    "remark-lint-no-blockquote-without-marker": "^2.0.1",
    "remark-lint-no-consecutive-blank-lines": "^1.0.1",
    "remark-lint-no-emphasis-as-heading": "^1.0.1",
    "remark-lint-no-file-name-outer-dashes": "^1.0.2",
    "remark-lint-no-heading-content-indent": "^1.0.1",
    "remark-lint-no-heading-indent": "^1.0.1",
    "remark-lint-no-heading-like-paragraph": "^1.0.1",
    "remark-lint-no-heading-punctuation": "^1.0.1",
    "remark-lint-no-inline-padding": "^1.0.1",
    "remark-lint-no-table-indentation": "^1.0.1",
    "remark-lint-ordered-list-marker-style": "^1.0.1",
    "remark-lint-ordered-list-marker-value": "^1.0.1",
    "remark-lint-strong-marker": "^1.0.1",
    "remark-lint-table-cell-padding": "^1.0.1",
    "remark-lint-table-pipe-alignment": "^1.0.1",
    "remark-lint-unordered-list-marker-style": "^1.0.1",
    "style-loader": "^0.20.2",
    "stylint": "^1.5.9",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^22.4.0",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.9.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "tslint-react": "^3.5.1",
    "webfontloader": "^1.6.28",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9",
    "webpack-dev-middleware": "^3.0.0",
    "webpack-hot-middleware": "^2.21.1"
  }
}
