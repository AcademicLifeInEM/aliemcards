{
  "name": "aliemcards",
  "version": "0.2.0",
  "description": "ALiEM Cards deployment scripts and website code.",
  "main": "./index.js",
  "author": "Jeremy Voros",
  "license": "ISC",
  "scripts": {
    "test": "jest --coverage && npm run lint",
    "dev": "gulp __build && torus run node ./index.js",
    "start": "NODE_ENV=production node ./index.js",
    "lint": "eslint -f codeframe \"app/**/*.{js,jsx}\" \"server/schema.js\" \"index.js\" && stylint app/assets/css",
    "s3": "aws s3 sync ./cards s3://aliemcards --acl public-read --exclude \".*\" --exclude \"*/.*\" --exclude \"*/*.md\" --delete; fi",
    "build": "gulp __build && NODE_ENV=production webpack",
    "now-build": "echo Build Skipped",
    "deploy": "torus run ./deploy"
  },
  "dependencies": {
    "axios": "^0.15.0",
    "body-parser": "^1.16.0",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "express-graphql": "^0.6.1",
    "fuse.js": "^2.6.1",
    "graphql": "^0.9.0",
    "helmet": "^3.4.0",
    "normalizr": "^3.1.0"
  },
  "devDependencies": {
    "autoprefixer-stylus": "^0.13.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.1",
    "del": "^2.2.2",
    "enzyme": "^2.7.1",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "front-matter": "^2.1.1",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-image": "^2.7.3",
    "gulp-stylus": "^2.6.0",
    "html-webpack-plugin": "^2.26.0",
    "jest": "^18.1.0",
    "marked": "^0.3.6",
    "now": "^3.0.0",
    "offline-plugin": "^4.5.5",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-router": "next",
    "style-loader": "^0.13.1",
    "stylint": "^1.5.9",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "svg-react-loader": "next",
    "webfontloader": "^1.6.27",
    "webpack": "beta",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0"
  },
  "jest": {
    "testRegex": "(/__tests__/.*\\.(js|jsx))$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "verbose": true,
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "!**/__tests__/**",
      "!app/**/index.{ts,tsx}",
      "!app/utils/**"
    ],
    "coverageDirectory": "tmp/coverage",
    "transform": {
      ".*": "./node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "./node_modules/react",
      "./node_modules/react-dom",
      "./node_modules/react-addons-test-utils"
    ]
  }
}
