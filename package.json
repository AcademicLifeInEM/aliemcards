{
  "name": "aliemcards",
  "version": "0.5.8",
  "description": "ALiEM Cards deployment scripts and website code.",
  "main": "./dist/index.js",
  "author": "Jeremy Voros",
  "license": "MIT",
  "scripts": {
    "pretest": "yarn lint",
    "prepush": "yarn build && yarn test",
    "test": "jest --coverage",
    "dev": "gulp __build && ts-node ./dist/index.js",
    "deploy": "torus run ./deploy",
    "start": "node ./dist/index.js",
    "lint": "yarn lint:typescript && yarn lint:stylus && yarn lint:markdown",
    "lint:markdown": "remark -q ./cards",
    "lint:typescript": "tslint --project . -t codeFrame",
    "lint:stylus": "stylint app/assets/css",
    "s3": "aws s3 sync ./cards s3://aliemcards --acl public-read --exclude \".*\" --exclude \"*/.*\" --exclude \"*/*.md\" --delete",
    "now-build": "echo build step skipped",
    "build": "gulp __build && NODE_ENV=production webpack"
  },
  "now": {
    "alias": [
      "aliemcards.com",
      "www.aliemcards.com"
    ],
    "name": "aliemcards",
    "env": {
      "NODE_ENV": "production"
    },
    "files": [
      "dist"
    ]
  },
  "dependencies": {
    "autotrack": "^2.3.3",
    "axios": "^0.16.0",
    "compression": "^1.6.2",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "fuse.js": "^3.0.4",
    "graphql": "^0.10.1",
    "helmet": "^3.6.1",
    "normalizr": "^3.2.3",
    "now": "^6.1.1",
    "react": "^15.5.4",
    "react-apollo": "^1.2.0",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.1.3",
    "react-router-dom": "^4.1.1",
    "remark": "^7.0.1",
    "remark-html": "^6.0.0",
    "typescript": "^2.3.3"
  },
  "devDependencies": {
    "@types/compression": "^0.0.33",
    "@types/express": "^4.0.35",
    "@types/express-graphql": "^0.0.33",
    "@types/helmet": "^0.0.35",
    "@types/jest": "^19.2.3",
    "@types/react": "^15.0.25",
    "@types/webpack": "^2.2.15",
    "autoprefixer-stylus": "^0.14.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.28.3",
    "del": "^2.2.2",
    "enzyme": "^2.8.2",
    "front-matter": "^2.1.2",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-image": "^2.9.0",
    "gulp-stylus": "^2.6.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.4",
    "jest": "^20.0.4",
    "js-yaml": "^3.8.3",
    "json-loader": "^0.5.4",
    "offline-plugin": "^4.8.1",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-svg-loader": "^1.1.1",
    "react-test-renderer": "^15.4.2",
    "remark-cli": "^3.0.1",
    "remark-lint": "^6.0.0",
    "remark-lint-blockquote-indentation": "^1.0.0",
    "remark-lint-emphasis-marker": "^1.0.0",
    "remark-lint-file-extension": "^1.0.0",
    "remark-lint-final-newline": "^1.0.0",
    "remark-lint-first-heading-level": "^1.0.0",
    "remark-lint-heading-increment": "^1.0.0",
    "remark-lint-heading-style": "^1.0.0",
    "remark-lint-list-item-bullet-indent": "^1.0.0",
    "remark-lint-list-item-content-indent": "^1.0.0",
    "remark-lint-list-item-indent": "^1.0.0",
    "remark-lint-no-auto-link-without-protocol": "^1.0.0",
    "remark-lint-no-blockquote-without-caret": "^1.0.0",
    "remark-lint-no-consecutive-blank-lines": "^1.0.0",
    "remark-lint-no-emphasis-as-heading": "^1.0.0",
    "remark-lint-no-heading-content-indent": "^1.0.0",
    "remark-lint-no-heading-indent": "^1.0.0",
    "remark-lint-no-heading-like-paragraph": "^1.0.0",
    "remark-lint-no-heading-punctuation": "^1.0.0",
    "remark-lint-no-inline-padding": "^1.0.0",
    "remark-lint-no-table-indentation": "^1.0.0",
    "remark-lint-ordered-list-marker-style": "^1.0.0",
    "remark-lint-ordered-list-marker-value": "^1.0.0",
    "remark-lint-strong-marker": "^1.0.0",
    "remark-lint-table-cell-padding": "^1.0.0",
    "remark-lint-table-pipe-alignment": "^1.0.0",
    "remark-lint-unordered-list-marker-style": "^1.0.0",
    "style-loader": "^0.18.1",
    "stylint": "^1.5.9",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-jest": "^20.0.4",
    "ts-loader": "^2.1.0",
    "ts-node": "^3.0.4",
    "tslint": "^5.3.2",
    "webfontloader": "^1.6.28",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.17.0"
  },
  "jest": {
    "globals": {
      "__TEST__": true
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*\\.(ts|tsx))$",
    "mapCoverage": true,
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json"
    ],
    "verbose": true,
    "collectCoverageFrom": [
      "app/**/*.{ts,tsx}",
      "!**/__tests__/**",
      "!app/**/index.{ts,tsx}",
      "!app/App.jsx",
      "!app/utils/**"
    ]
  }
}
