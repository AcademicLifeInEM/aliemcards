{
  "name": "aliemcards",
  "version": "0.5.11",
  "description": "ALiEM Cards deployment scripts and website code",
  "license": "MIT",
  "main": "./dist/index.js",
  "author": "Jeremy Voros",
  "scripts": {
    "build": "gulp __build && NODE_ENV=production webpack",
    "deploy": "torus run ./deploy",
    "dev": "gulp __build && ts-node ./dist/index.js",
    "lint": "yarn lint:typescript && yarn lint:stylus && yarn lint:markdown",
    "lint:markdown": "remark -q ./cards",
    "lint:stylus": "stylint app/assets/css",
    "lint:typescript": "tslint --type-check -p . -t codeFrame && tslint --type-check -p ./app -t codeFrame",
    "now-build": "echo build step skipped",
    "precommit": "lint-staged",
    "prepush": "yarn build && yarn test -- --no-coverage",
    "pretest": "yarn lint",
    "prettier": "prettier --write --single-quote --trailing-comma all --print-width 100 \"**/*.{ts,tsx}\"",
    "s3": "aws s3 sync ./cards s3://aliemcards --acl public-read --exclude \".*\" --exclude \"*/.*\" --exclude \"*/*.md\" --delete",
    "start": "node ./dist/index.js",
    "test": "jest"
  },
  "now": {
    "alias": [
      "aliemcards.com",
      "www.aliemcards.com"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "files": [
      "dist"
    ],
    "name": "aliemcards"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write --single-quote --trailing-comma all --print-width 100",
      "git add"
    ]
  },
  "jest": {
    "globals": {
      "__TEST__": true
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*\\.(ts|tsx))$",
    "mapCoverage": true,
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json"
    ],
    "verbose": true,
    "collectCoverageFrom": [
      "app/**/*.{ts,tsx}",
      "!**/__tests__/**",
      "!app/**/index.{ts,tsx}",
      "!app/App.jsx",
      "!app/utils/**"
    ]
  },
  "dependencies": {
    "autotrack": "^2.4.1",
    "axios": "^0.16.2",
    "compression": "^1.7.0",
    "express": "^4.15.3",
    "express-graphql": "^0.6.7",
    "fuse.js": "^3.0.5",
    "graphql": "^0.10.5",
    "gulp": "gulpjs/gulp#4.0",
    "helmet": "^3.8.1",
    "normalizr": "^3.2.3",
    "now": "^7.2.4",
    "optics-agent": "^1.1.6",
    "react": "^15.6.1",
    "react-apollo": "1.2.0",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-router-dom": "^4.1.2",
    "remark": "^8.0.0",
    "remark-html": "^6.0.1",
    "typescript": "^2.4.2"
  },
  "devDependencies": {
    "@types/compression": "^0.0.33",
    "@types/express": "^4.0.36",
    "@types/express-graphql": "^0.0.33",
    "@types/helmet": "^0.0.35",
    "@types/jest": "^20.0.5",
    "@types/react": "^15.6.0",
    "@types/webpack": "^3.0.5",
    "autoprefixer-stylus": "^0.14.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "enzyme": "^2.9.1",
    "front-matter": "^2.1.2",
    "gulp-image": "^2.10.0",
    "gulp-stylus": "^2.6.0",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "js-yaml": "^3.9.0",
    "json-loader": "^0.5.7",
    "lint-staged": "^4.0.2",
    "offline-plugin": "^4.8.3",
    "prettier": "^1.5.3",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "^1.3.1",
    "react-svg-loader": "^1.1.1",
    "react-test-renderer": "^15.6.1",
    "remark-cli": "^4.0.0",
    "remark-lint": "^6.0.0",
    "remark-lint-blockquote-indentation": "^1.0.0",
    "remark-lint-emphasis-marker": "^1.0.0",
    "remark-lint-file-extension": "^1.0.0",
    "remark-lint-final-newline": "^1.0.0",
    "remark-lint-first-heading-level": "^1.1.0",
    "remark-lint-heading-increment": "^1.0.0",
    "remark-lint-heading-style": "^1.0.0",
    "remark-lint-linebreak-style": "^1.0.0",
    "remark-lint-list-item-bullet-indent": "^1.0.0",
    "remark-lint-list-item-content-indent": "^1.0.0",
    "remark-lint-list-item-indent": "^1.0.0",
    "remark-lint-no-auto-link-without-protocol": "^1.0.0",
    "remark-lint-no-blockquote-without-caret": "^1.0.0",
    "remark-lint-no-consecutive-blank-lines": "^1.0.0",
    "remark-lint-no-emphasis-as-heading": "^1.0.0",
    "remark-lint-no-file-name-outer-dashes": "^1.0.1",
    "remark-lint-no-heading-content-indent": "^1.0.0",
    "remark-lint-no-heading-indent": "^1.0.0",
    "remark-lint-no-heading-like-paragraph": "^1.0.0",
    "remark-lint-no-heading-punctuation": "^1.0.0",
    "remark-lint-no-inline-padding": "^1.0.0",
    "remark-lint-no-table-indentation": "^1.0.0",
    "remark-lint-ordered-list-marker-style": "^1.0.0",
    "remark-lint-ordered-list-marker-value": "^1.0.0",
    "remark-lint-strong-marker": "^1.0.0",
    "remark-lint-table-cell-padding": "^1.0.0",
    "remark-lint-table-pipe-alignment": "^1.0.0",
    "remark-lint-unordered-list-marker-style": "^1.0.0",
    "style-loader": "^0.18.2",
    "stylint": "^1.5.9",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-jest": "^20.0.7",
    "ts-loader": "^2.3.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.5.0",
    "webfontloader": "^1.6.28",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2"
  }
}
