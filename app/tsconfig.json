{
  "compilerOptions": {
    "allowJs": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "dom.iterable",
      "es2017",
      "es2017.object"
    ],
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "target": "esnext",
    "sourceMap": true
  }
}
