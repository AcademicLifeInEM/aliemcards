{
  "extends": "tslint:latest",
  "rules": {
    "arrow-parens": false,
    "interface-name": [ false ],
    "member-access": [ false ],
    "no-angle-bracket-type-assertion": false,
    "no-var-requires": false,
    "object-literal-key-quotes": [ false ],
    "object-literal-sort-keys": false,
    "quotemark": [ false ],
    "semicolon": [ false ],
    "variable-name": [ false ],
    "whitespace": [ false ]
  }
}
